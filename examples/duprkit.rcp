src="duprkit"
version="0.git"
description="Debian User Recipe Toolkit"
srcurl="https://github.com/dupr/duprkit/archive/master.tar.gz"
license="MIT"
section="utils"
maintainer="Debian User Recipe Toolkit Developers"
depends="python3 (>= 3.6~),"

dk_prep_source () {
	mkdir $src
	tar xf ${src}_${version}.tar.gz --strip-components=1 -C $src
	rm -rf $src/debian
}

^ debian/rules 0o755
#!/usr/bin/make -f
export DESTDIR=$(shell pwd)/debian/duprkit/
export PREFIX=/usr
%:
	dh $@

override_dh_auto_test:
